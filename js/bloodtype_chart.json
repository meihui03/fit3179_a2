{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Stacked bar chart showing blood donations by location and blood type",
    "width": 300,
    "height": 400,
    "background": "transparent" ,
    "data": {
      "url": "https://raw.githubusercontent.com/meihui03/fit3179_a2/refs/heads/main/data/preprocessed_donations_for_radar_chart.csv",
      "format": {"type": "csv"}
    },
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "Blood_Type",
        "type": "nominal",
        "title": "Blood Type",
        "axis": {"labelAngle": 0}
      },
      "y": {
        "field": "Total_Donations",
        "type": "quantitative",
        "title": "Total Donations"
      },
      "color": {
        "field": "Location",
        "type": "nominal",
        "title": "Location"
      }
    }
  }
  